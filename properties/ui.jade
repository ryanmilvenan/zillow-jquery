h5 Search
//
    .row
        .col-md-8
            .form-group.form-group-default
                label Name            
                input#name.form-control(type='text', required='')                
        .col-md-4                         
            button.btn.btn-primary(onClick='legislators.searchByName($("#name").val())') Search


.row
    .col-md-8
        .form-group.form-group-default
            label Address            
            input#address.form-control(type='text', required='')                
        .form-group.form-group-default
            label Zipcode            
            input#zipcode.form-control(type='text', required='')                
    .col-md-4                         
        button.btn.btn-primary(onClick='properties.searchByZipcode($("#address").val(), $("#zipcode").val())') Search
