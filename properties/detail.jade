- var street = data[2].children[0].textContent
- var city = data[2].children[2].textContent
- var state = data[2].children[3].textContent
- var zip = data[2].children[1].textContent
- var address = street + "\n" + city+", " + state + " " + zip
- function monetize(n, currency) { return currency + " " + n.toFixed(2).replace(/(\d)(?=(\d{3})+\.)/g, "$1,"); }
- var u_price = data[3].children[0].textContent
- var price = monetize(parseInt(u_price), "$")
- var c_price = data[3].children[3].textContent
- c_price= monetize(parseInt(c_price), "$")
- var z_details = data[1].textContent

h2= address
    .row
        .col-md-6
            h4.subtitle Estimated Price:
            = price
    .row
        .col-md-6
            h4.subtitle Change In Price Over 30 Days:
            = c_price
    .row
        .col-md-6
            h4.subtitle
                a(href=z_details) More Info
            
